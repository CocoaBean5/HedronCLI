{
  "alias": "hedron.phar",
  "chmod": "0755",
  "directories": [
    "src"
  ],
  "files": [
    "LICENSE.txt",
    "bin/hedron.php",
    "vendor/autoload.php"
  ],
  "finder": [
    {
      "name": "/(\\.php|\\.json|\\.yml|\\.twig)$/",
      "exclude": ["Tests","tests", "docs"],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Php"
  ],
  "compression": "GZ",
  "main": "bin/hedron.php",
  "output": "hedron.phar",
  "stub": true
}